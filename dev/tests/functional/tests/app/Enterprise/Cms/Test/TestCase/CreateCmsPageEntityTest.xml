        </variation>
        <variation name="CreateCmsPageEntityTestVariation3" method="test">
            <data name="cms/data/under_version_control" xsi:type="string">No</data>
            <data name="notFoundMessage" xsi:type="string">WE ARE SORRY, BUT THE PAGE YOU ARE LOOKING FOR CANNOT BE FOUND.</data>
            <data name="isIFrame" xsi:type="boolean">true</data>
        </variation>
        <variation name="CreateCmsPageEntityTestVariation4">
            <data name="cms/data/under_version_control" xsi:type="string">No</data>
