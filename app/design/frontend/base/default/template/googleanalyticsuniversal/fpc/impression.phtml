/** @var Enterprise_GoogleAnalyticsUniversal_Block_List_Json $this */
$item = $this->getFpcBlock()->getItem();
?>

<?php if (is_object($item)): ?>
<script type="text/javascript">

updatedImpressions = updatedImpressions || [];
updatedImpressions['<?php echo $this->getBlockName() ?>'] = updatedImpressions['<?php echo $this->getBlockName() ?>'] || [];

updatedImpressions['<?php echo $this->getBlockName() ?>'] =
    updatedImpressions['<?php echo $this->getBlockName() ?>'].concat ([
    {
        "id":"<?php echo $this->jsQuoteEscape($item->getSku()) ?>",
        "type":"<?php echo $item->getTypeId() ?>",
        "name":"<?php echo $this->jsQuoteEscape($item->getName()); ?>",
        "category":"<?php echo $this->jsQuoteEscape($this->getCurrentCategoryName()) ?>",
        "list":"<?php echo $this->jsQuoteEscape($this->getCurrentListName()) ?>",
        "position": updatedImpressions['<?php echo $this->getBlockName() ?>'].length + 1,
        "listPosition": updatedImpressions['<?php echo $this->getBlockName() ?>'].length
    }
]);
</script>
<?php endif?>
